			GROUP_CONCAT( ttt.term_id ) AS tags
		, e.start as start, e.end as end, e.allday 
			FROM wp_ai1ec_events e
				LEFT JOIN wp_term_relationships tr
					ON ( e.post_id = tr.object_id )
				LEFT JOIN wp_term_taxonomy ttc
					ON (
						tr.term_taxonomy_id = ttc.term_taxonomy_id AND
						ttc.taxonomy = 'events_categories'
					)
				LEFT JOIN wp_term_taxonomy ttt
					ON (
						tr.term_taxonomy_id = ttt.term_taxonomy_id AND
						ttt.taxonomy = 'events_tags'
					)
				
			WHERE e.post_id = 256
			GROUP BY e.post_id made by require('wp-blog-header.php'), require_once('wp-includes/template-loader.php'), do_action('template_redirect'), call_user_func_array, Ai1ec_Front_Controller->route_request, Ai1ec_Command->execute, Ai1ec_Command_Render_Event->do_execute, Ai1ec_Registry_Object->get, Ai1ec_Registry_Object->dispatch, Ai1ec_Factory_Event->create_event_instance, Ai1ec_Event->__construct, Ai1ec_Event->initialize_from_id, Ai1ec_Dbi->get_row
